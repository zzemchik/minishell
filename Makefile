all: $(NAME) #jepa2

%.o: %.c
	@gcc -c $< -o $@ -I ./headers

$(NAME): $(OSOURCE)
	@gcc -g $(OSOURCE) -o $(NAME)
	@rm -rf $(OSOURCE)

